[Desktop Action tippse]
Exec=konsole -e /builds/tippse/tippse %F
Icon=application-x-object
Name=Open with tippse
Name[it]=Apri con tippse

[Desktop Action dhex]
Exec=konsole -e dhex %F
Icon=application-x-object
Name=Open with dhex
Name[it]=Apri con dhex
